#include <iostream>                                                                                                                                                                                                
using namespace std;    
int main(){    
  double a[4];    
  for (int i = 0; i < 4; ++i){    
        cin >> a[i];    
  }    
  double A = (a[0] + a[2]) / 2;    
  double B = (a[1] + a[3]) / 2;    
  double C = (a[3] - B);    
    int _A=(int)A;    
    int _B=(int)B;    
    int _C=(int)C;    
  if (A >= 0 && B >= 0 && C >= 0    
       &&A==(double)_A&&B==(double)_B&&C==(double)_C    
       ){    
    cout << A << " " << B << " " << C << endl;    
  }    
  else{    
    cout << "No" << endl;    
  }    
}    
    
