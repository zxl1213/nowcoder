#include <iostream>
using namespace std;
int Fibonacci(int n){
    if(n==0){
        return 0;
    }else if (n == 1 || n == 2){
		return 1;
	}
	else{
		return Fibonacci(n - 1) + Fibonacci(n - 2);
	}
}
int main(){
	int n = 0, N;
	cin >> N;
	while (N>Fibonacci(n)){
		n++;
	}
	int ret= Fibonacci(n) - N>N-Fibonacci(n - 1) ? N-Fibonacci(n - 1) : Fibonacci(n)-N;
	cout << ret << endl;
}
